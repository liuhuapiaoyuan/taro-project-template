{
  "extends": ["taro/react"],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "indent": [
      "error",
      2
    ],
    "react/self-closing-comp": [
      "error"
    ],
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "Taro"
      }
    ]
  }
}
